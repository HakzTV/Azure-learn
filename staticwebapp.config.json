{
  "routes": [
    {
      "route": "/search"
    },
    {
      "route": "/upload",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/view/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/.auth/*", "/api/*" ]
  }
}
