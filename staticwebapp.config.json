{
  "routes": [
    {
      "route": "/search",
      "rewrite": "/index.html"
    },
    {
      "route": "/upload",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    },
    {
      "route": "/view/*",
      "allowedRoles": ["authenticated"],
      "rewrite": "/index.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/.auth/*", "/api/*" ]
  }
}
