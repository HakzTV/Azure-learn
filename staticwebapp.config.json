{
  "routes": [
    {
      "route": "/search",
      "methods": ["GET"],
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/upload",
      "methods": ["GET"],

      "allowedRoles": ["authenticated"],
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/view/*",
      "methods": ["GET"],

      "allowedRoles": ["authenticated"],
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
      "methods": ["GET"],

    "exclude": [ "/.auth/*", "/api/*" ]
  }
}
